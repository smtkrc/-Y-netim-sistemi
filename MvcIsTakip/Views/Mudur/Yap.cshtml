
@{
    ViewBag.Title = "Yap";
    Layout = "~/Views/Shared/_LayoutMudur.cshtml";
}
<br />
<br />
<br />
<h2>Yap</h2>
<div class="container">
    <div class="jumbotron">

    </div>
    <div>
        @foreach (var item in ViewBag.isler)
        {
                using (Html.BeginForm("Yap", "Mudur", FormMethod.Post))
                {
                    <div class="card">
                        <div class="card-body">
                            <input type="hidden"id="IsId" name="IsId" value="@item.IsID" />
                            @item.IsAciklama
                        </div>
                        <div class="card-footer">
                            <button type="submit" class="btn btn-success">Yapıldı</button>
                        </div>
                    </div>
                }
        }
    </div>
</div>
