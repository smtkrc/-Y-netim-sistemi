
@{
    ViewBag.Title = "Yap";
    Layout = "~/Views/Shared/_LayoutMemur.cshtml";
}
<br />
<br />
<br />
<h2>Yap</h2>
<div class="container">
    <div class="jumbotron">

    </div>
    <div>
        @foreach (var item in ViewBag.isler)
        {
                using (Html.BeginForm("Yap", "Memur", FormMethod.Post))
                {
                    <div class="card">
                        <div class="card-body">
                            <input type="hidden" name="IsID" value="@item.IsID" />
                            @item.IsAciklama
                        </div>
                        <div>
                            <button class="btn btn-success" type="submit">Yapıldı</button>
                        </div>
                    </div>
                }
        }
    </div>
</div>
