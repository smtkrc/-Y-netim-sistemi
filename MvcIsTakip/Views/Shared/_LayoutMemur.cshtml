
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>_LayoutMudur</title>
</head>
<body>
    <nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">İş Takip</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        @Html.ActionLink("Index", "Index", "Memur", new { @class = "nav-link active" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("İş Yap", "Yap", "Memur", new { @class = "nav-link active" })
                    </li>
                    <li class="nav-item">
                        @Html.ActionLink("İş Takip", "Takip", "Memur", new { @class = "nav-link active" })
                    </li>
                </ul>

            </div>
        </div>
    </nav>
    <main role="main" class="container">
        @RenderBody()
    </main>
</body>
</html>
